{
  "name": "gkd-mrlc",
  "version": "2.0.126",
  "type": "module",
  "gkd": {
    "outDir": "dist",
    "file": "beta-gkd.json5",
    "versionFile": "beta-gkd.version.json5",
    "changelog": "CHANGELOG.md",
    "README.md": "README.md"
  },
  "main": "./dist/gkd.json5",
  "scripts": {
    "postinstall": "simple-git-hooks",
    "format": "prettier --cache --write ./**/*.{js,cjs,mjs,ts,jsx,tsx,json}",
    "lint": "eslint --cache --fix ./**/*.{js,cjs,mjs,ts,jsx,tsx}",
    "check": "tsc --noEmit && tsx ./scripts/check.ts",
    "build": "tsc --noEmit && tsx ./scripts/build.ts"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "pre-push": "pnpm run check"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,cts,mts,jsx,tsx}": [
      "prettier --cache --write",
      "eslint --cache --fix"
    ],
    "*.json": [
      "prettier --cache --write"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@eslint/js": "9.24.0",
    "@gkd-kit/api": "0.7.1",
    "@gkd-kit/define": "0.0.1",
    "@gkd-kit/tools": "0.6.0",
    "@types/node": "22.14.1",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-unused-imports": "4.1.4",
    "json5": "2.2.3",
    "lint-staged": "15.5.1",
    "prettier": "3.5.3",
    "simple-git-hooks": "2.12.1",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1"
  },
  "volta": {
    "node": "22.14.0",
    "pnpm": "10.8.0"
  },
  "packageManager": "pnpm@10.8.1",
  "engineStrict": true,
  "engines": {
    "node": ">=22"
  }
}
